<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab7.controller.RegisterController">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutY="14.0" prefHeight="32.0" prefWidth="400.0" text="Register">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="42.0" layoutY="132.0" prefHeight="18.0" prefWidth="324.0" text="First Name" />
        <Label layoutX="42.0" layoutY="191.0" prefHeight="18.0" prefWidth="324.0" text="Last Name" />
        <Label layoutX="42.0" layoutY="250.0" prefHeight="18.0" prefWidth="324.0" text="Email" />
        <Label layoutX="42.0" layoutY="308.0" prefHeight="18.0" prefWidth="324.0" text="Password" />
        <TextField fx:id="firstNameTField" layoutX="42.0" layoutY="151.0" prefHeight="26.0" prefWidth="324.0" promptText="First Name" />
        <TextField fx:id="lastNameTField" layoutX="42.0" layoutY="210.0" prefHeight="26.0" prefWidth="324.0" promptText="Last Name" />
        <TextField fx:id="emailTField" layoutX="42.0" layoutY="268.0" prefHeight="26.0" prefWidth="324.0" promptText="Email" />
        <PasswordField fx:id="passwordPasswordField" layoutX="42.0" layoutY="327.0" prefHeight="26.0" prefWidth="324.0" promptText="Password" />
        <Button fx:id="registerButton" layoutX="42.0" layoutY="416.0" mnemonicParsing="false" onAction="#registerButtonClicked" prefHeight="26.0" prefWidth="324.0" text="Register" />
        <Label fx:id="registerErrorLabel" layoutX="42.0" layoutY="66.0" prefHeight="51.0" prefWidth="324.0" textFill="RED" wrapText="true" />
    </children>
</AnchorPane>
