<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab7.controller.MyRequestsController">
    <children>
        <TableView fx:id="requestsTable" layoutX="25.0" layoutY="21.0" prefHeight="400.0" prefWidth="400.0">
            <columns>
                <TableColumn fx:id="tableColumnFirstName" prefWidth="100.0" text="FirstName" />
                <TableColumn fx:id="tableColumnLastName" prefWidth="100.0" text="LastName" />
                <TableColumn fx:id="tableColumnDate" prefWidth="100.0" text="Date" />
                <TableColumn fx:id="tableColumnStatus" prefWidth="100.0" text="Status" />
            </columns>
        </TableView>
        <Button fx:id="deleteButton" layoutX="26.0" layoutY="448.0" mnemonicParsing="false" onAction="#handleDeleteRequest" prefHeight="26.0" prefWidth="400.0" text="Delete Request" />
    </children>
</AnchorPane>
